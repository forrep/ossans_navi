        },
        {
            "name": "起動（production）",
            "type": "debugpy",
            "request": "launch",
            "program": "ossans_navi/app.py",
            "console": "integratedTerminal",
            "args": [
                "--production"
            ]
        },
        {
            "name": "Pytest",
            "type": "debugpy",
            "request": "launch",
            "module": "pytest",
            "console": "integratedTerminal",
            "justMyCode": true
