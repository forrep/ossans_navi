            "program": "ossans_navi/app.py",
            "console": "integratedTerminal",
            "args": []
        },
        {
            "name": "起動（dev-gemini）",
            "type": "debugpy",
            "request": "launch",
            "program": "ossans_navi/app.py",
            "console": "integratedTerminal",
            "args": [],
            "env": {
                "OSN_AI_SERVICE_TYPE": "gemini"
            }
        },
        {
            "name": "起動（dev-gemini-pro）",
            "type": "debugpy",
            "request": "launch",
            "program": "ossans_navi/app.py",
            "console": "integratedTerminal",
            "args": [],
            "env": {
                "OSN_AI_SERVICE_TYPE": "gemini",
                "OSN_MODEL_HIGH_QUALITY": "gemini-3-pro-preview",
                "OSN_MODEL_HIGH_QUALITY_IN": "2.00",
                "OSN_MODEL_HIGH_QUALITY_OUT": "12.00"
            }
        },
        {
            "name": "起動（dev-azure_openai）",
            "type": "debugpy",
            "request": "launch",
            "program": "ossans_navi/app.py",
            "console": "integratedTerminal",
            "args": [],
            "env": {
                "OSN_AI_SERVICE_TYPE": "azure_openai",
                "OSN_AI_MODEL_LOW_COST": "gpt-4.1-mini",
                "OSN_AI_MODEL_LOW_COST_IN": "0.40",
                "OSN_AI_MODEL_LOW_COST_OUT": "1.60",
                "OSN_AI_MODEL_HIGH_QUALITY": "gpt-4.1",
