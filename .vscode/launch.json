    "version": "0.2.0",
    "configurations": [
        {
            "name": "起動（dev）",
            "type": "debugpy",
            "request": "launch",
            "program": "ossans_navi/app.py",
            "console": "integratedTerminal",
            "args": []
        },
        {
            "name": "起動（dev-gemini-pro）",
            "type": "debugpy",
            "request": "launch",
            "program": "ossans_navi/app.py",
            "console": "integratedTerminal",
            "args": [],
            "env": {
                "OSN_AI_MODEL_LOW_COST": "GEMINI_20_FLASH",
                "OSN_AI_MODEL_HIGH_QUALITY": "GEMINI_25_PRO"
            }
        },
        {
            "name": "起動（dev-azure_openai）",
            "type": "debugpy",
            "request": "launch",
            "program": "ossans_navi/app.py",
            "console": "integratedTerminal",
            "args": [],
            "env": {
                "OSN_AI_MODEL_LOW_COST": "AZURE_GPT_41_MINI",
                "OSN_AI_MODEL_HIGH_QUALITY": "AZURE_GPT_41"
            }
        },
        {
            "name": "起動（production）",
            "type": "debugpy",
            "request": "launch",
            "program": "ossans_navi/app.py",
            "console": "integratedTerminal",
            "args": [
                "--production"
            ]
        },
        {
            "name": "Pytest",
            "type": "debugpy",
            "request": "launch",
            "module": "pytest",
            "console": "integratedTerminal",
            "justMyCode": true
        }
    ]
}